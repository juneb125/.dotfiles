{
  # navigation
  ".." = "cd ..";
  "..." = "cd ../..";

  # 'ls'
  la = "ls -aG";
  ll = "ls -lG";
  lal = "ls -alG";

  # git
  ga = "git add";
  gb = "git branch";
  gd = "git diff";
  gl = "git log --graph";
  gp = "git push";
  gs = "git status";

  # vim / nvim
  nv = "nvim";
  # vi -> nvim alias handled in neovim.viAlias
  # vim -> nvim alias handled in neovim.vimAlias

  # misc.
  c = "clear";
  e = "exit 0";
  scerr = "scutil --error";
}
