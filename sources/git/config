# This is Git's per-user configuration file.
[user]
	name = June B
	email = jrbergeron823@gmail.com
	username = juneb125

[init]
	defaultBranch = main

[core]
	# global .gitignore
	excludesFile = $HOME/.gitignore

[alias]
	br = branch
	cam = commit -am
	cl = clone
	cm = commit -m
	co = checkout
	i = init
	log = log --graph

[push]
	default = current
	followTags = true

[pull]
	default = current

[github]
	user = juneb125
	# token updated 11/20/24
	token = ${JUNES_GITHUB_TOKEN}

[credential]
	helper = store

[url "https://github.com/"]
	insteadOf = "github:"

